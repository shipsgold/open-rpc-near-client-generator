{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2020",
      "dom"
    ],
    "allowJs": true,
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck":true,

  },
  "exclude": [
    "generated-client",
    "build",
    "templates"
  ]
}